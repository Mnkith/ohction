<div class="container col-9" >
<h3>Your purchased items</h3>
  <%# <%= display_user_purchases%>
  <% if current_user.purchases%>
    <table class="table table-hover">
      <thead>
        <tr>
          <%# <th scope="col">#</th> %>
          <th scope="col">Item</th>
          <th scope="col">Price</th>
          <th scope="col">Bought on</th>
        </tr>
      </thead>
      <tbody>
        <%= render partial: 'users/purchase', collection: current_user.purchases %>
      </tbody>
    </table>
  <% else %>
    <h5>You dont any purchaces</h5>
  <% end %>
</div>
